{
  "name": "fnorden.net",
  "version": "1.0.0",
  "description": "Der Vorschlag für fnorden.net, für den Fall, dass auch anderen 15 HTML Tags zu primitiv sind",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:bootstrap:css": "cp node_modules/bootstrap/dist/css/bootstrap.min.css css/",
    "build:bootstrap:fonts": "cp node_modules/bootstrap/dist/fonts/* fonts/",
    "build:bootstrap:js": "cp node_modules/bootstrap/dist/js/bootstrap.min.js js/",
    "build:font-awesome": "rm -rf vendor/font-awesome; mkdir vendor/font-awesome; cp -r node_modules/font-awesome/css/ vendor/font-awesome/css/; cp -r node_modules/font-awesome/fonts/ vendor/font-awesome/fonts/;",
    "build:jquery": "cp node_modules/jquery/dist/jquery.min.js js/jquery.js",
    "build:leaflet": "rm -rf vendor/leaflet; cp -r node_modules/leaflet/dist/ vendor/leaflet/",
    "css:less": "lessc less/grayscale.less css/grayscale.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/freifunkh/fnorden.net.git"
  },
  "author": "FNorden",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/freifunkh/fnorden.net/issues"
  },
  "homepage": "https://github.com/freifunkh/fnorden.net#readme",
  "devDependencies": {
    "bootstrap": "^3.4.1",
    "font-awesome": "^4.2.0",
    "jquery": "^3.7.1",
    "leaflet": "^0.7.7",
    "less": "^4.2.1"
  }
}
